{{!-- The tag below means - insert everything in this file into the {body} of the default.hbs template --}}
{{!< default}}

{{!-- The main content area --}}
<main id="content" role="main">

	<div class="container">
		<div class="row">
			<div class="col-lg-8">
				<div class="loop-medium">
					{{#foreach posts}}
		    			{{> "loop-medium"}}
				    {{/foreach}}
				</div>
				{{pagination}}
			</div>
            <div class="col-lg-4">
                {{> "sidebar"}}
            </div>
		</div>
	</div>

</main>